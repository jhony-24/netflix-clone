    "gatsby-plugin-react-helmet": "^3.5.0",
    "gatsby-plugin-styled-components": "^3.6.0",
    "gatsby-source-filesystem": "^2.6.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.1.0",
    "react-icons": "^4.1.0",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.3",
    "styled-components": "^5.2.1"
  },
  "devDependencies": {
    "faker": "^5.1.0",
    "gatsby-plugin-postcss": "^3.2.0",
    "gatsby-plugin-root-import": "^2.0.5",
    "gh-pages": "^3.1.0",
    "postcss": "^8.1.10",
    "prettier": "2.1.2",
